# Gateway Client 配置示例文件
# 复制此文件为 .env 并根据实际环境修改配置

# ============================================================================
# 基础配置
# ============================================================================

LOCAL_IP=127.0.0.1
GATEWAY_PORT=2381

# ============================================================================
# ETCD 配置
# ============================================================================

ETCD_HOST=localhost
ETCD_PORT=2379

# ============================================================================
# SSH 服务配置
# ============================================================================

SSH_PORT_START=40000
SSH_PORT_END=40099

# ============================================================================
# HTTP 服务配置
# ============================================================================

HTTP_ENDPOINT=example.com
SSH_ENDPOINT=connect.example.com

# ============================================================================
# FRP 配置
# ============================================================================

FRP_HOST=localhost
FRP_PORT=7400
FRP_USERNAME=admin
FRP_PASSWORD=123456

# ============================================================================
# Kong API Gateway 配置
# ============================================================================

KONG_ADMIN_URL=http://127.0.0.1:8001

# ============================================================================
# CORS 配置
# ============================================================================

ENABLE_CORS=0
